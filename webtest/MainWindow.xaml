<Window x:Class="webtest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:webtest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="textBox" Margin="10,0,10,7" TextWrapping="Wrap" Text="TextBox" Height="23" VerticalAlignment="Bottom"/>
        <WebBrowser x:Name="webBrowser" Margin="10,10,10,35" Source="http://www50.zippyshare.com/v/1VHXCUpf/file.html" LoadCompleted="webBrowser_LoadCompleted" Navigated="webBrowser_Navigated"/>

    </Grid>
</Window>
